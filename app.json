{
  "expo": {
    "name": "Hometown Specials",
    "slug": "hometown-specials",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#56DF6C",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7AC149"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hmspecials.hometownspecials"
    },
    "android": {
      "versionCode": 8,
      "package": "com.kinghorn.hometownspecials",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
